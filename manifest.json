{
  "manifest_version": 2,

  "name": "Osmote",
  "description": "This extension changes all words or phrases on the web into whatever the user wants",
  "version": "1.0.1",
  "background": {
  "scripts": ["content-1.js"],
  "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs"
  ]
}